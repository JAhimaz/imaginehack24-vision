{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "./src/",
    "paths": {
      "@/components/*": ["components/*"],
      "@/components": ["components"],
      "@/hooks/*": ["hooks/*"],
      "@/hooks": ["hooks"],
      "@/util/*": ["util/*"],
      "@/util": ["util"],
      "@/libs/*": ["libs/*"],
      "@/libs": ["libs"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/Profile/Dialogs"],
  "exclude": ["node_modules"]
}
